    <%- include('partials/header') %>

    <div class="row justify-content-center">
    <div class="col-md-7 col-lg-5">
        <div class="card">
        <div class="card-body text-center">
            <h2 class="h5 mb-2">Search Result</h2>
            <p class="<%= found ? 'text-success' : 'text-danger' %> fw-semibold"><%= message %></p>

            <% if (found && data) { %>
            <div class="mt-3 text-start">
                <div class="d-flex justify-content-between border rounded-3 p-3 mb-2" style="border-color: var(--stroke)">
                <span class="text-muted">Name</span>
                <span class="fw-semibold"><%= data.description %></span>
                </div>
                <div class="d-flex justify-content-between border rounded-3 p-3" style="border-color: var(--stroke)">
                <span class="text-muted">Base Total</span>
                <span class="badge-soft"><%= data.base_total %></span>
                </div>
            </div>
            <% } else { %>
            <p class="text-muted mt-3 mb-0">Try another name from the home page.</p>
            <% } %>

            <a href="/" class="btn btn-outline-primary mt-4">Back to Home</a>
        </div>
        </div>
    </div>
    </div>

    <%- include('partials/footer') %>